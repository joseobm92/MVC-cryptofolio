
    <div class="container mt-5">
        <table class="table ">
            <thead>
                <tr>
                    <th scope="col">Rank</th>
                    <th scope="col">Name</th>
                    <th scope="col">Price</th>
                    <th scope="col">Market Cap.</th>
                </tr>
            </thead>
            {{#each data as |cryptocurrency| }}
            <tbody>
                <tr>
                    <th scope="row">{{cryptocurrency.rank}}</th>
                    <td>{{cryptocurrency.name}} {{cryptocurrency.symbol}} </td>
                    <td> {{cryptocurrency.stat.price}}</td>
                    <td>{{cryptocurrency.stat.market_cap}}</td>
                </tr>
                
            </tbody>
            {{/each}}
        </table>
    </div>

    
    
