
    <div class="container mt-5">
        <table class="table ">
            <thead>
                <tr>
                    <th scope="col">Rank</th>
                    <th scope="col">Name</th>
                    <th scope="col">Price</th>
                    <th scope="col">Market Cap.</th>
                </tr>
            </thead>
            {{#each data as |cryptocurrency| }}
            <tbody>
                <tr>
                    <th scope="row">{{cryptocurrency.cmc_rank}}</th>
                    <td><a href="/coin/{{cryptocurrency.cmc_rank}}">{{cryptocurrency.name}} ({{cryptocurrency.symbol}})</a></td>
                    <td> {{cryptocurrency.quote.USD.price}}</td>
                    <td>{{cryptocurrency.quote.USD.market_cap}}</td>
                </tr>
                
            </tbody>
            {{/each}}
        </table>
    </div>

    
    
