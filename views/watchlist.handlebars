
    <div class="container mt-5">
        <table class="table custom-tbl">
            <thead>
                <tr>
                    <th scope="col">Rank(#) </th>
                    <th scope="col">Name </th>
                    <th scope="col">Price($) </th>
                    <th scope="col">Market Cap($) </th>
                    <th scope="col">Circulating Supply </th>
                    <th scope="col">Max Supply </th>
                    <th scope="col"></th>
                </tr>
            </thead>
            {{#each cryptocurrencies as |cryptocurrency| }}
            <tbody>
                <tr>
                    <th scope="row">{{cryptocurrency.rank}}</th>
                    <td><a href="/coin/{{cryptocurrency.rank}}">{{cryptocurrency.name}} ({{cryptocurrency.symbol}})</a></td>
                    <td> {{toPrecision cryptocurrency.price 7}}</td>
                    <td>{{addCommas (toPrecision cryptocurrency.market_cap 12)}}</td>
                    <td>{{addCommas (toPrecision cryptocurrency.circulating_supply 12)}}</td>
                    <td>{{addCommas (toPrecision cryptocurrency.max_supply 12)}}</td>
                    <td> <button class="{{cryptocurrency.symbol}} btn-danger rounded delete-btn">Delete</button></td>
                </tr>
                
            </tbody>
            {{/each}}
        </table>
    </div>

    <script src="/js/delete.js"></script>